page {
  background: #eee;
}

.h_wrap {
  margin-top: 0;
}
.jsd{
  width:100%;
  height:100rpx;
}
.heads {
  width: 92%;
  padding:0 4%;
  position: fixed;
  top:0;
  left:0;
  background: #eee;
  z-index: 99;
}

.heads text {
  font-weight: bold;
  display: inline-block;
  width: 21.33%;
  margin: 0 6%;
  color: #4f5f66;
  text-align: center;
  font-size: 28rpx;
  height: 96rpx;
  line-height: 96rpx;
}

.heads .green {
  color: #27cc23;
  border-bottom: 4rpx solid #27cc23;
}

.goodlists {
  padding: 20rpx 0;
  margin-top: 20rpx;
  border-radius: 10rpx;
  background: #fff;
}

.goods {
  width: 120rpx;
  height: 120rpx;
  margin: 20rpx;
}

.weo {
  width: 57%;
  font-size: 28rpx;
}

.goodsname {
  color: #4f5f66;
  font-weight: bold;
  margin: 10rpx 0;
}

.goodssum {
  color: #4f5f66;
  margin: 10rpx 0;
}

.goodstime {
  color: #8d9fa7;
  font-size: 24rpx;
}

.shenhe {
  width: 18%;
  align-items: center;
  display: flex;
  justify-content: center;
}

.shentext {
  color: #fd5e24;
  font-size: 24rpx;
  text-align: right;
}

.point {
  display: block;
  width: 8rpx;
  height: 8rpx;
  margin-left: 12rpx;
  margin-right: 1%;
  margin-top: 5rpx;
  border-top: 4rpx solid #d1d1d1;
  border-right: 4rpx solid #d1d1d1;
  transform: rotate(45deg);
}
