page {
  background: #efefef;
}

.present-contans {
  display: flex;
  flex-direction: column;
  width: 702rpx;
  margin: auto;
  font-family: "微软雅黑";
  color: #4f5f66;
}

.present-list-box {
  width: 100%;
  height: 152rpx;
  background: #fff;
  border-radius: 10rpx;
  margin-top: 25rpx;
  font-size: 28rpx;
  position: relative;
}

.mention-wechat-box {
  margin: 25rpx 0 0 22rpx;
  height: 40rpx;
  line-height: 40rpx;
}

.present-state {
  margin: 25rpx 0 0 22rpx;
}

.present-time {
  height: 40rpx;
  line-height: 40rpx;
  position: absolute;
  right: 22rpx;
  top: 50%;
  font-size: 24rpx;
  color: #8d9fa7;
  margin-top: -20rpx;
}

.metion-weight {
  font-weight: 600;
}

.wechat-monty {
  font-size: 34rpx;
  font-family: "Arial";
}

.review-success {
  color: #27cc23;
}
.review-ing{
  color:#EE6723;
}
.review-reject{
  color:#FF4E6E;
}
.review-in {
  color: #fd5e24;
}
.data-nothing{
  width: 240rpx;
  height: 240rpx;
  margin: auto;
  margin-top: 200rpx;
}
.data-text{
  text-align: center;
  font-size: 28rpx;
  height: 40rpx;
  line-height: 40rpx;
}
