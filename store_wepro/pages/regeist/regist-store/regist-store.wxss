.regist-contances{
    width: 702rpx;
    display: flex;
    flex-direction: column;
    vertical-align: middle;
    font-size: 28rpx;
    margin: 0 auto;
    color: #4F5F66;
}

.regist-titel{
    display: flex;
    height: 40rpx;
    line-height: 40rpx;
    font-weight: 700;
    margin: 33rpx  0 0 24rpx;
}
.modular-box{
    height: 88rpx;
    line-height: 88rpx;
    width: 100%;
    background: #F8F8F8;
    border-radius: 10rpx;
    overflow: hidden;
    margin-top: 33rpx;
}
.modular-box label{
    display: block;
    float: left;
    width: 200rpx;
    padding-left: 25rpx;
}
.modular-box input{
    height: 88rpx;
    line-height: 88rpx;
    outline: 0;
    float: left;
    width: 450rpx;
    font-size: 14px;
    -webkit-appearance: none;
}
.placeh-style{
   color: #8D9FA7;
   font-size: 14px;
   font-family: "微软雅黑";
}
.modular-box input.verit-code{
    width: 250rpx;
}
.get-code{
    line-height: 88rpx;
    float: right;
    color: #586C94;
    padding: 0 25rpx;
}
.modular-box input.setPassword{
    width: 300rpx;
}
.submit-success-btn{
    width: 100%;
    height: 88rpx;
    text-align: center;
    line-height: 88rpx;
    background: #8AD988;
    border-radius: 10rpx;
    color: #fff;
    margin: 33rpx 0;
    font-size: 28rpx; 
}
