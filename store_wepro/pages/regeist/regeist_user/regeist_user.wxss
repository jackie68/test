.modle-register-box{
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    background: #EFEFEF;
    border-radius: 10rpx;
    position: relative;
    margin-top: 10rpx;
    margin-bottom: 10rpx;
    
}
.modle-register-box input{
    height: 88rpx;
    width: 80%;
    float: left;
    line-height: 88rpx;
    padding: 0 18rpx;
    font-size: 14px;
    font-family: "微软雅黑";
}
.modle-register-box input.shorten-input{
    width: 60%;
}
.clear-close{
    top: 23rpx;
}
.get-code-btn{
    top: 0;
}
